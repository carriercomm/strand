<pre><%= @dpm || '' %></pre>
<%= @permanode.blobref %><br />
<%= @permanode.blobcontent %><br />
<br />
<% if @content %>
	<h2>Content of this Permanode</h2>
	<%= @content.title %><br />
	<%= @content.body %>
<% end %>
<br /><br />
<% if @claims.count > 0 %>
	<h2>Revisions</h2>
	<% @claims.each do |claim| %>
	<%= claim.blobref %><br />
	<%= claim.blobcontent %><br /><br />
	<% end %>
<% end %>
<a href="/claim/create/<%= @permanode.blobref %>">Create New Claim</a><br /><br />

